<div>
  <div class="textpart">
    <h1>{{ section.settings.heading }}</h1>
    <p class="inercontent">{{ section.settings.paragraph }}</p>
  </div>
</div>

{% schema %}
{
  "name": "Text Section",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "WRIST TO WRIST.CUFF IN DESIRE:"
    },
    {
      "type": "textarea",
      "id": "paragraph",
      "label": "Paragraph Text",
      "default": "Lorem ipsum dolor sit amet, consectetur adipisicing elit. Quidem aspernatur voluptatum tempore, eos eaque corrupti totam cupiditate ex sint maxime doloremque ad necessitatibus illo vel sit dolore optio laudantium soluta."
    }
  ],
  "presets": [
    {
      "name": "Default",
      "category": "Custom"
    }
  ]
}
{% endschema %}
