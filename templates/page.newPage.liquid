{% layout none %}

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <!-- <link rel="stylesheet" href="style.css"> -->
    <style>
      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        font-family: "Gill Sans", "Gill Sans MT", Calibri, "Trebuchet MS",
          sans-serif;
      }

      .main-container {
        width: 100vw;
      }

      .one {
        width: 100%;
        height: 350px;
        background-color: rgb(14, 27, 36);
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
      }

      .paraOne,
      .paraTwo {
        color: white;
        font-size: 25px;
        font-weight: bolder;
        text-align: center;
      }

      .two {
        background-color: white;
        width: 100%;
        height: auto;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        text-align: center;
        padding: 20px;
      }

      .paraThree {
        font-size: 25px;
        font-weight: 900;
      }

      .paraFour {
        color: rgb(78, 78, 78);
        margin-top: 10px;
      }

      .three {
        height: auto;
        width: 100%;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        padding: 20px;
      }

      .three-left {
        height: 80%;
        width: 100%;
        margin-bottom: 20px;
      }

      .three-right {
        height: auto;
        width: 100%;
      }

      .para {
        margin-top: 20px;
        margin-bottom: 20px;
        color: rgb(78, 78, 78);
      }

      .paraFive {
        margin-top: 20px;
        margin-bottom: 20px;
        font-size: larger;
        font-weight: 900;
      }

      .para span {
        margin-left: 10px;
        font-weight: 900;
        color: black;
      }

      .shopping {
        height: 30px;
        width: 120px;
        background-color: black;
        color: white;
        border: none;
        border-radius: 16px;
        cursor: pointer;
        display: flex;
        align-items: center;
        justify-content: center;
        margin: 20px auto;
      }

      .four {
        width: 100%;
        height: 400px;
        background-color: black;
        display: flex;
        align-items: center;
        justify-content: center;
      }

      .four iframe {
        width: 100%;
        height: 100%;
        max-width: 600px;
        max-height: 300px;
      }

      .five {
        width: 100%;
        height: auto;
        background-color: rgb(107, 202, 232);
        display: flex;
        flex-direction: column;
        color: white;
        padding: 20px;
      }

      .five-left {
        width: 100%;
        display: flex;
        align-items: center;
        justify-content: center;
        padding: 20px;
      }

      .five-left img {
        width: 80%;
        height: auto;
      }

      .five-right {
        width: 100%;
        display: flex;
        flex-direction: column;
        justify-content: center;
        padding: 20px;
      }

      .right {
        margin-top: 20px;
        font-weight: bolder;
      }

      .five-right p {
        font-weight: lighter;
        margin-right: 20px;
      }

      .heading {
        font-size: 30px;
        /* text-align: center; */
      }

      @media (min-width: 768px) {
        .main-container {
          height: auto;
        }

        .three {
          flex-direction: row;
        }

        .three-left,
        .three-right {
          height: 80%;
          width: 50%;
          margin-bottom: 0;
        }

        .five {
          flex-direction: row;
          height: 600px;
        }

        .five-left {
          width: 50%;
        }

        .five-right {
          width: 100%;
        }

        .paraOne,
        .paraTwo {
          font-size: 25px;
        }

        .paraThree {
          font-size: 25px;
        }

        .paraFour {
          width: 50%;
          font-size: 18px;
        }

        .paraFive {
          font-size: 24px;
        }

        .para {
          font-size: 18px;
          margin-right: 0;
        }

        .shopping {
          height: 40px;
          width: 150px;
        }

        .heading {
          font-size: 40px;
        }

        .four iframe {
          width: 40%;
          height: 60%;
        }
      }

      @media (max-width: 768px) {

        .one,
        .two,
        .three,
        .four,
        .five {
          padding: 10px;
        }

        .paraOne,
        .paraTwo {
          font-size: 20px;
        }

        .paraThree {
          font-size: 20px;
        }

        .paraFour {
          font-size: 14px;
          width: 100%;
        }

        .paraFive {
          font-size: 18px;
        }

        .para {
          font-size: 14px;
          margin-right: 0;
        }

        .shopping {
          height: 30px;
          width: 100px;
        }

        .heading {
          font-size: 25px;
        }

        .new_Image {
          width: 80%;
          height: 60%;
        }
      }

      .slider {
        position: relative;
        width: 100%;
        max-width: 400px;
        margin: auto;
        overflow: hidden;
      }

      .slides {
        display: flex;
        transition: transform 0.5s ease-in-out;
      }

      .slide {
        min-width: 100%;
        height: 300px;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 2em;
        color: white;
      }

      button {
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        background-color: rgba(0, 0, 0, 0.5);
        border: none;
        color: white;
        font-size: 2em;
        padding: 0.5em;
        cursor: pointer;
        z-index: 10;
      }

      .prev {
        left: 10px;
      }

      .next {
        right: 10px;
      }
    </style>
  </head>

  <body>
    <div class="main-container">
      {% section 'testbanner1' %}
      {% section 'testbanner2' %}

      <div class="three">
        <div class="three-left">
          <div class="slider">
            <div class="slides">
              <div class="slide">
                <img
                  src="data:image/jpeg;base64,"
                  alt=""
                >
              </div>
              <div class="slide">
                <img
                  src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQsGcngrMGlXiiJITcW2VvgTS_Y0dSJIemUrQ&s"
                  alt=""
                >
              </div>
              <div class="slide">
                <img
                  src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRcQ5aULg3WcJm0QJMJ-olk9HagB3s5cD1sMg&s"
                  alt=""
                >
              </div>
            </div>
            <button class="prev">&#10094;</button>
            <button class="next">&#10095;</button>
          </div>
        </div>

        <div class="three-right">
          {% section 'testbanner3-right' %}
          <div class="shopping">SHOP NOW</div>
        </div>
      </div>

      <div class="four">
        {% section 'testbanner4' %}
      </div>

      <div class="five">
        <div class="five-left">
          {% section 'testbanner5Image' %}
        </div>
        <div class="five-right">
          {% section 'testbanner5' %}
        </div>
      </div>
    </div>

    <script>
    const slides = document.querySelector(".slides");
    const slide = document.querySelectorAll(".slide");
    const prevButton = document.querySelector(".prev");
    const nextButton = document.querySelector(".next");

    let index = 0;

    function showSlide(index) {
      const slideWidth = slide[index].clientWidth;
      slides.style.transform = `translateX(${-index * slideWidth}px)`;
    }

    prevButton.addEventListener("click", () => {
      index = index > 0 ? index - 1 : slide.length - 1;
      showSlide(index);
    });

    nextButton.addEventListener("click", () => {
      index = index < slide.length - 1 ? index + 1 : 0;
      showSlide(index);
    });

    // Optional: Auto-slide functionality
    setInterval(() => {
      index = index < slide.length - 1 ? index + 1 : 0;
      showSlide(index);
    }, 3000);
    </script>
  </body>
</html>
